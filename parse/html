          chat: {
            jwtFn: function(callback) {
              fetch('/zendesk_widget_sso').then(function(res) {
                  res.text().then(function(jwt) {
                    callback(jwt);
                });
              })
            }
          }
        }
      }
    }

    zE(function() {
      if (!zE('webWidget:get', 'chat:isChatting')) zE.hide();
    });

    $(document).on("click", "a.zendesk-widget", function() {
      zE.activate({ hideOnClose: true });
    });
